@using ShoppersSquare_proto.Models

@{
    ICollection<ProductType> categories = Model;
}

@{
    Html.RenderPartial("_CheckoutButton");

    Html.RenderPartial("_MegaBanner", new ProductType()
    {
        Name = "Shoppers Square",
        ImageUrl = "/Content/Images/various products.jpeg"
    });
}

@foreach (var category in categories)
{
    Html.RenderPartial("_CardsContainer", category);
}